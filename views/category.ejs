        footer {
            background-color: #333;
            color: #fff;
            padding: 20px;
        }

        .socail-media {
            display: flex;
            justify-content: space-between;
            align-items: center;
        }

        ul {
            list-style: none;

        }

        li {
            display: inline-block;
            margin-left: 20px;
        }

        .card-text {
            padding: 0 0 10px 0;
        }
    </style>

    <!-- Heading -->
